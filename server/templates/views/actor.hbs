<body>
    <h2 class="mt-4 text-center">Movies of {{name.leadactor}}</h2>
    <table class="table">
        <thead>
          <tr>
            
            <th scope="col">Movie Name</th>
            <th scope="col">leadacter</th>
            <th scope="col">actress</th>
            <th scope="col">director</th>
            <th scope="col">year of release</th>
          </tr>
        </thead>
        <tbody>
            {{#movies}}
          <tr>
            <th >{{name}}</th>
            <td>{{leadactor}}</td>
            <td>{{actress}}</td>
            <td>{{director}}</td>
             <td>{{year}}</td>
          </tr>
          {{/movies}}
        </tbody>
             </table>
</body>
</html>